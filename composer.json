{
    "name": "embarknow/http",
    "description": "HTTP related classes",
    "license": "MIT",
    "authors": [
        {
            "name": "John Porter",
            "email": "john@embarknow.co.uk"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "git",
            "url": "git://github.com/embarknow/http"
        }
    ],
    "require": {
        "php": ">=5.6",
        "psr/http-message": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "EmbarkNow\\Http\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EmbarkNow\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.1"
    }
}
